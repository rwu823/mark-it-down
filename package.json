{
  "name": "mark-it-down",
  "version": "0.2.2",
  "description": "The modern Markdown parser",
  "main": "./dist/mark-it-down.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot",
    "build": "npm run build.lib & npm run build.demo",
    "build.lib": "NODE_ENV=build babel-node ./scripts/build.lib",
    "build.demo": "NODE_ENV=demo webpack --progress",
    "test": "jest src/ --no-cache --coverage",
    "publish": "cd npm && npm publish",
    "coveralls": "cat coverage/lcov.info | coveralls"
  },
  "jest": {
    "coveragePathIgnorePatterns": ["create-stylesheet"],
    "moduleNameMapper": {
      "^.+\\.(s?css|svg|png|jpg|gif)$": "<rootDir>/test/mock-loader"
    }
  },
  "keywords": [
    "markdown",
    "mark",
    "down",
    "it",
    "parser",
    "parse",
    "morden",
    "code",
    "syntax",
    "highlight",
    "prism",
    "showdown"
  ],
  "author": "Rocky Wu <rocky823@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-external-helpers-2": "^6.3.13",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "coveralls": "^2.11.13",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.1.0",
    "gulp-uglify": "^2.0.0",
    "html-webpack-plugin": "^2.22.0",
    "jest-cli": "^15.1.1",
    "node-sass": "^3.10.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "prismjs": "^1.5.1",
    "showdown": "^1.4.3"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rwu823/mark-it-down.git"
  },
  "bugs": {
    "url": "https://github.com/rwu823/mark-it-down/issues"
  },
  "homepage": "https://github.com/rwu823/mark-it-down#readme"
}
